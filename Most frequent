import java.util.*;

class Solution {
public int maxFrequency(int[] nums, int k) {

 Arrays.sort(nums);
 int n = nums.length;
 int maxFreq = 1;

for(int i = 0; i < n; i++) {
int operations = k;
int count = 1;

for(int j = i - 1; j >= 0; j--) {
int diff = nums[i] - nums[j];
 if(operations >= diff) {
   operations -= diff;
   count++;
  } else {
    break;
        }
        }
 if(count > maxFreq) {
   maxFreq = count;
            }
        }
  return maxFreq;
    }
}
